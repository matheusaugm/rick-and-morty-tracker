  "compilerOptions": {
    "types": ["cypress", "@testing-library/cypress"],
    "target": "ES2015",
    "lib": ["ES2015", "DOM"],
    "noEmit": true,
    "isolatedModules": false,
    "skipLibCheck": true,
    "strict": false
